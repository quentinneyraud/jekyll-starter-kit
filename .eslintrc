{
  "extends": [
    "standard"
  ],
  "plugins": [
    "dependencies"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "DEVELOPMENT": false,
    "PRODUCTION": false,
    "ENVIRONMENT": false
  },
  "rules": {
    "dependencies/case-sensitive": 1,
    "arrow-parens": 0,
    "no-undef": 0
  }
}
