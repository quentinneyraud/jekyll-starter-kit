{
  "name": "jekyll-starter",
  "version": "1.0.0",
  "description": "A simple boilerplate for webpack and jekyll integration.",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently './node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --host=0.0.0.0' 'JEKYLL_ENV=development jekyll serve --watch'",
    "build": "./node_modules/webpack/bin/webpack.js && JEKYLL_ENV=production jekyll build"
  },
  "author": "Quentin Neyraud",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "cssnano": "^3.8.1",
    "debug": "^2.3.3",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "barba.js": "^1.0.0",
    "domready": "^1.0.8",
    "gsap": "^1.19.0",
    "webfontloader": "^1.6.26",
    "webpack-dev-server": "^1.16.2"
  }
}
